{
  "name": "cognigy-actions",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:sdk": "cd sdk-app && npm run build",
    "build": "npm run build:sdk && vue-tsc -b && vite build",
    "build:gh-pages": "npm run build:sdk && cross-env GITHUB_PAGES=true VITE_GITHUB_PAGES=true vue-tsc -b && cross-env GITHUB_PAGES=true VITE_GITHUB_PAGES=true vite build",
    "preview": "vite preview",
    "deploy": "npm run build:gh-pages && gh-pages -d dist"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "vue": "^3.5.24",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  }
}
